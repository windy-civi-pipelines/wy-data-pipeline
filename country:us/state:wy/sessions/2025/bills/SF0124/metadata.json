{
  "legislative_session": "2025",
  "identifier": "SF0124",
  "title": "Illegal immigration-identify, report, detain and deport.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to immigration; establishing offenses related to the transportation, concealment or hiring of illegal or unauthorized aliens; providing penalties; requiring the verification of citizenship status for public benefits as specified; requiring the attorney general and each county sheriff to negotiate with federal agencies concerning immigration as specified; specifying requirements for communicating information related to immigration status; specifying requirements for the determination of immigration status for inmates; requiring documentation concerning citizenship for the issuance of driver's licenses and identification cards; providing and amending definitions; requiring reports; specifying applicability; requiring rulemaking; and providing for effective dates."
    }
  ],
