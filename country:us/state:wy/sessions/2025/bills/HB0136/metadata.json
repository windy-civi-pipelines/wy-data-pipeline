{
  "legislative_session": "2025",
  "identifier": "HB0136",
  "title": "Volunteer first responder health insurance-revisions.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
      "note": "summary",
