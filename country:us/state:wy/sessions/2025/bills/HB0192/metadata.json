  "identifier": "HB0192",
  "title": "Public utilities-wildfire mitigation and liability limits.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [
    {
